    <!-- 获取用户列表 分页-->
    <select id="getItripProductStorePage" resultType="cn.itrip.beans.pojo.ItripProductStore" parameterType="map">
        SELECT
                    id as id,
                    productType as productType,
                    productId as productId,
                    store as store,
                    creationDate as creationDate,
                    createdBy as createdBy,
                    modifyDate as modifyDate,
                    modifiedBy as modifiedBy
        FROM
        itrip_product_store table1
